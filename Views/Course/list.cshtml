@Model List<Course>

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <div>
        @foreach(var kurs in Model)
        {
            <h2> @kurs.Title </h2>
            <p> @kurs.Description  </p>
            <img src="~/img/@kurs.Image" alt="">
        }

    </div>
</body>
</html>